{
  "name": "react-node-scripts",
  "version": "1.0.0",
  "description": "A command line tool to create react components or utilities",
  "repository": "https://github.com/dabitCoder/react-node-scripts.git",
  "author": "<doliva@redpoints.com>",
  "license": "MIT",
  "main": "./src/index.js",
  "bin": {
    "generate": "./src/index"
  },
  "scripts": {
    "prompt": "babel-node ./src/index.js",
    "build": "rm -rf ./dist && cp -r ./src/index.js dist"
  },
  "keywords": [
    "cli",
    "create-project"
  ],
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@octokit/rest": "^16.28.7",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "configstore": "^5.0.0",
    "figlet": "^1.2.3",
    "inquirer": "^7.0.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "simple-git": "^1.124.0",
    "touch": "^3.1.0"
  }
}
