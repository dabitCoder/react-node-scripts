{
  "name": "react-node-scripts",
  "version": "1.0.0",
  "description": "A command line tool to create react components or utilities",
  "repository": "https://github.com/dabitCoder/react-node-scripts.git",
  "author": "<doliva@redpoints.com>",
  "license": "MIT",
  "main": "./src/index.js",
  "preferGlobal": true,
  "bin": {
    "call-generator": "./lib/commands.js"
  },
  "scripts": {
    "prompt": "babel-node ./src/index.js",
    "commands": "babel-node ./lib/commands.js",
    "test": "jest"
  },
  "keywords": [
    "cli",
    "create-project"
  ],
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.6.2",
    "@octokit/rest": "^16.28.7",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "commander": "^4.0.1",
    "configstore": "^5.0.0",
    "esm": "^3.2.25",
    "figlet": "^1.2.3",
    "handlebars": "^4.5.1",
    "inquirer": "^7.0.0",
    "java-class-tools": "^1.3.1",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "prettier": "^1.18.2",
    "simple-git": "^1.124.0",
    "touch": "^3.1.0"
  }
}
